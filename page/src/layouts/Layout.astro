---
interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Astro description" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body>
		<div class="h-10 bg-blue-500">
			Preheader
		</div>
		<header class="sticky h-20 bg-orange-400 top-0"></header>
		<slot />
	</body>
</html>
<style is:global>
	 @font-face {
    font-family: 'MyFont';
    font-style: normal;
    font-weight: 400;
    font-display: swap;
    src: url(/fonts/RubikWetPaint-Regular.ttf) format('woff2');
  }

</style>
